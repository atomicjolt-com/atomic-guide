{
  "private": true,
  "version": "1.3.3",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "preview": "npm run build && vite preview",
    "check": "tsc && vite build && node scripts/inject-manifest.js && wrangler deploy --dry-run",
    "build": "tsc -b && vite build && node scripts/inject-manifest.js",
    "deploy": "npm run build && wrangler deploy",
    "test": "vitest",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint-fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "tail": "wrangler tail --format pretty",
    "types": "wrangler types",
    "db:create": "wrangler d1 create atomic-guide-db",
    "db:destroy": "wrangler d1 delete atomic-guide-db",
    "db:reset": "npm run db:destroy && npm run db:create && npm run db:migrate && npm run db:seed",
    "db:migrate": "node scripts/db-migrate.js migrate",
    "db:migrate:remote": "node scripts/db-migrate.js migrate --remote",
    "db:rollback": "node scripts/db-migrate.js rollback",
    "db:status": "node scripts/db-migrate.js status",
    "db:seed": "tsx scripts/seed.ts --run",
    "db:seed:remote": "tsx scripts/seed.ts --run --remote",
    "db:setup": "node scripts/d1-setup.cjs",
    "db:list": "wrangler d1 execute atomic-guide-db --command=\"SELECT name FROM sqlite_master WHERE type='table' ORDER BY name\"",
    "db:query": "node scripts/d1-query.cjs",
    "db:export": "wrangler d1 export atomic-guide-db --output=backup.sql",
    "db:local": "wrangler d1 execute atomic-guide-db --local --command",
    "kv:setup": "node scripts/kv-setup.cjs",
    "kv:destroy": "node scripts/kv-cleanup.cjs"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.12.0",
    "@cloudflare/vitest-pool-workers": "^0.8.67",
    "@cloudflare/workers-types": "^4.20250826.0",
    "@eslint/js": "^9.34.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jsdom": "^21.1.7",
    "@types/katex": "^0.16.0",
    "@types/prismjs": "^1.26.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitejs/plugin-react": "^5.0.0",
    "abort-controller": "^3.0.0",
    "eslint": "^9.34.0",
    "eslint-config-typescript": "^3.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jsdom": "^26.1.0",
    "jsonc-parser": "^3.3.1",
    "msw": "^2.10.5",
    "mustache": "^4.2.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vitest": "3.1.4",
    "vitest-fetch-mock": "^0.4.5",
    "wrangler": "^4.33.0"
  },
  "dependencies": {
    "@atomicjolt/lti-client": "^2.7.0",
    "@atomicjolt/lti-components": "^1.2.0",
    "@atomicjolt/lti-endpoints": "^3.3.0",
    "@atomicjolt/lti-server": "^3.3.2",
    "@atomicjolt/lti-types": "^1.2.7",
    "@cloudflare/ai": "^1.2.2",
    "@reduxjs/toolkit": "^2.8.2",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.8",
    "dompurify": "^3.2.2",
    "hono": "^4.9.4",
    "i18next": "^25.4.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "jose": "^6.0.13",
    "jsonwebtoken": "^9.0.2",
    "katex": "^0.16.18",
    "prismjs": "^1.29.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-intersection-observer": "^9.14.0",
    "react-redux": "^9.2.0",
    "uuid": "^11.1.0",
    "xss": "^1.0.15"
  },
  "name": "atomic-guide"
}
