{
  "private": true,
  "version": "1.3.3",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "preview": "npm run build && vite preview",
    "check": "tsc && vite build && node scripts/inject-manifest.js && wrangler deploy --dry-run",
    "build": "tsc -b && vite build && node scripts/inject-manifest.js",
    "deploy": "npm run build && wrangler deploy",
    "test": "vitest",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "tail": "wrangler tail --format pretty",
    "types": "wrangler types",
    "db:setup": "chmod +x scripts/setup-d1.sh && ./scripts/setup-d1.sh",
    "db:seed": "wrangler d1 execute atomic-guide-db-preview --file=src/db/seed.sql",
    "db:query": "wrangler d1 execute atomic-guide-db-preview --command"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.11.7",
    "@cloudflare/vitest-pool-workers": "^0.8.66",
    "@cloudflare/workers-types": "^4.20250821.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jsdom": "^21.1.7",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitejs/plugin-react": "^5.0.0",
    "abort-controller": "^3.0.0",
    "eslint": "^9.33.0",
    "eslint-config-typescript": "^3.0",
    "jsdom": "^26.1.0",
    "msw": "^2.10.5",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vitest": "3.1.4",
    "vitest-fetch-mock": "^0.4.5",
    "wrangler": "^4.32.0"
  },
  "dependencies": {
    "@atomicjolt/lti-client": "^2.7.0",
    "@atomicjolt/lti-endpoints": "^3.3.0",
    "@atomicjolt/lti-server": "^3.3.2",
    "@atomicjolt/lti-types": "^1.2.7",
    "@reduxjs/toolkit": "^2.8.2",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "hono": "^4.9.1",
    "i18next": "^25.4.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "jose": "^6.0.13",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-redux": "^9.2.0",
    "uuid": "^11.1.0"
  },
  "name": "atomic-guide"
}
